{
  "name": "tochka-developers/jsonrpc-client",
  "description": "JsonRpc Client for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Babushkin",
      "email": "darkdarin@tochka.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "illuminate/log": ">=5.6",
    "illuminate/cache": ">=5.6",
    "illuminate/config": ">=5.6",
    "illuminate/container": ">=5.6",
    "illuminate/console": ">=5.6",
    "illuminate/support": ">=5.6",
    "illuminate/pipeline": ">=5.6",
    "tochka-developers/jsonrpc-smd": "^1.0",
    "phpdocumentor/reflection-docblock": "~4.3",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tochka\\JsonRpcClient\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tochka\\JsonRpcClient\\JsonRpcClientServiceProvider"
      ]
    }
  }
}
